  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web (local)",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "args": [
        "--web-port", "8888",
      ]
    },
    {
      "name": "Web (local, profile mode)",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "args": [
        "--web-port", "8888",
      ],
      "flutterMode": "profile"
    },
    {
      "name": "Web (local, release mode)",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "args": [
        "--web-port", "8888",
      ],
      "flutterMode": "release"
    }
  ]
}